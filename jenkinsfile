pipeline{
	agent none
	stages
	{
		stage('build')
		{
			steps{
			echo "build stage is working...."
			}
		}
		stage('test')
		{
			steps{
			echo "test stage is working"
			}
		}
		stage('deploy')
		{
			agent{
				docker {
      					image 'ubuntu:latest'
      					label 'ubuntu-agent'
			}
			steps{
				sh 'echo "Hello"'
				sh 'sudo apt-get update'
			}
		}
	}
}
